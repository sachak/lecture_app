{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "function",     // nécessite la function key côté serveur SWA
      "type": "httpTrigger",       // déclencheur HTTP
      "direction": "in",
      "name": "req",
      "methods": [ "post" ],       // on accepte uniquement les requêtes POST
      "route": "save_results"      // URL : /api/save_results
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}
